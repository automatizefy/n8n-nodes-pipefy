{
  "name": "n8n-nodes-pipefy",
  "version": "0.1.1",
  "description": "Nó n8n para integração com a API do Pipefy",
  "keywords": [
    "n8n-community-node-package",
    "pipefy",
    "n8n",
    "workflow",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/automatizefy/n8n-nodes-pipefy",
  "author": {
    "name": "Automatizefy",
    "email": "contato@automatizefy.com.br"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/automatizefy/n8n-nodes-pipefy.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Pipefy.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Pipefy/Pipefy.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^5.0.0",
    "n8n-workflow": "^1.70.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
